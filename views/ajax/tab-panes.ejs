<% var userType = user.__proto__.constructor.name %>

<div class="tab-pane fade" id="nav-catalog" role="tabpanel" aria-labelledby="nav-catalog-tab">
    <%- include("../partials/catalog",{flowers: data.flowers}) %>
</div>
<% if(userType === "Employee" || userType === "Manager"){ %>
    <div class="tab-pane fade" id="nav-users" role="tabpanel" aria-labelledby="nav-users-tab">
        <%- include( "../partials/users") %>
    </div>
<% } %>
<% if(userType === "Manager"){ %>
    <div class="tab-pane fade" id="nav-employees" role="tabpanel" aria-labelledby="nav-employees-tab">
        <%- include( "../partials/employees") %>
    </div>
<% } %>

<% if(userType === "Manager"){ %>
    <div class="tab-pane fade" id="nav-branches" role="tabpanel" aria-labelledby="nav-branches-tab">
        <%- include( "../partials/branches") %>
    </div>
<% } %>